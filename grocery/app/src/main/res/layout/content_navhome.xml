<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:card_view="http://schemas.android.com/tools"
    android:background="#f58600"
    android:orientation="vertical"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:showIn="@layout/app_bar_navhome"
    android:padding="10dp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:padding="25dp"
        android:background="#fff" >
<TextView
    android:id="@+id/txtemail"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="Email"
    ></TextView>
    <TextView
        android:id="@+id/txtuser"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Name"></TextView>
    <androidx.cardview.widget.CardView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elevation="5dp"
        android:layout_margin="5dp"
        android:layout_gravity="center"
        card_view:cardCornerRadius="@dimen/card_movie_radius"
        >

        <ImageView

            android:id="@+id/imageView22"
            android:layout_width="337dp"
            android:layout_height="139dp"
            app:srcCompat="@drawable/blue2" />
    </androidx.cardview.widget.CardView>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="20dp"
        >

        <LinearLayout
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:background="@drawable/ffff" >



            <TextView
                android:id="@+id/text_veg"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="VEGGIES"

                android:layout_gravity="center"
                android:background="#80000000"
                android:onClick="veggies"
                android:textColor="#f58600"
                android:textSize="30dp" />

        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="150dp"
            android:layout_marginLeft="10dp"
            android:background="@drawable/vegnew" >

            <TextView
                android:id="@+id/text_fruit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="FRUITS"
                android:layout_gravity="center"
                android:background="#80000000"
                android:onClick="fruit"
                android:textSize="30dp"
                android:textColor="#f58600"
                />

        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"

        android:paddingTop="20dp">

        <LinearLayout
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:background="@drawable/bak"
           >
            <TextView
                android:id="@+id/text_bak"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="BAKERY"
                android:layout_gravity="center"
                android:background="#80000000"
                android:onClick="bakery"
                android:textAlignment="center"
                android:textSize="30dp"
                android:textColor="#f58600"
                />


        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:background="@drawable/fd"
            android:paddingLeft="10dp">


            <TextView
                android:id="@+id/text_grain"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="FOOD GRAIN"
                android:layout_gravity="center"
                android:background="#80000000"
                android:onClick="foodgrain"
                android:textAlignment="center"
                android:textSize="30dp"
                android:textColor="#f58600"
                />




        </LinearLayout>

    </LinearLayout>

</LinearLayout>

</LinearLayout>